---
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../../consts';
import { getCollection } from 'astro:content';
import FormattedDate from '../../components/FormattedDate.astro';


---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
		<style>

			li {
				display: flex;
				width: 350px;
				padding: 10px;
				background-color: rgba(242, 242, 242, 0.185);
				border-radius: 2px;
				border: 1px solid rgba(235, 235, 235, 0.719);


			}

			main {
				width: 100%;
				height: 70vh;
				


			}
			ul {
        display: flex;
        flex-wrap: wrap;
        justify-content: center; /* Center horizontally */
        padding-inline-start: 0px;
        gap: 10px;
    }


			
			a {
				display: flex;
			}

			img {
				max-width: 100%;
				height: auto;
				border-radius: 4px;
				margin: 0px;
				padding: 0px;
				margin-left: -5px;
				border: 1px solid rgba(169, 169, 169, 0.288);

			}

			.info, h4, p, a, .down {
				margin: 0px;
				font-family: -apple-system, system-ui, BlinkMacSystemFont, "Segoe UI", Roboto, Ubuntu;
				text-decoration: none;
				margin-left: 5px;
			}

			.down {
				color: gray;
				margin-top: 6px;
				font-weight: 300;
				display: flex;
				align-items: center;
				gap: 5px;
			}

			h6 {
				font-weight: 400;
				margin: 0px
			}

			hr {
				background-color: black;
				width: 1px;
				height: 10px;



			}
			.role {

			}

			h4{
				font-size: 20px;
                color: gray;
			}

			p {
				font-size: 12px
			}

			section {


			}

			h2 {
				font-family: 'Charter';
				font-size: 20px;
				color: rgb(160, 160, 160);
			}

			.toptopt {
				display: flex;
			}

			h3 {
				font-size: 16px;
				font-family: 'Charter';
				margin-bottom: 3px;

			}


            .center {
                width: 100%;
                text-align: center;
                margin-top: 20vh;

            }


		</style>
	</head>
	<body>
		<Header />
		<main>
			<section>
				<div class="center">

				<h4>Nothing to see here</h4>
				</div>
			</section>
		</main>
		<Footer />
	</body>
</html>